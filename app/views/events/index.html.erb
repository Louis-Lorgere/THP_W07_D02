<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Welcome to Eventbrite</h1>
    <p class="lead">Projet de simili Eventbrite dans la formation THP</p>
    
  </div>
</div>
<div class="album py-5 bg-light">
  <div class="container">
  <%@events.each do |event|%>
    <div class="card">
      <div class="card-body">
        <h5 class="card-title"><%=event.title%></h5>
        <p class="card-text"><%=event.description%></p>
        <%= link_to "Show Event", event_path(event.id), class: "btn btn-primary" %>
      </div>
    </div>
  <%end%>
  </div>
</div>
